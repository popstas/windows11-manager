{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-create",
    "core:window:allow-close",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-kill",
    "global-shortcut:default",
    "global-shortcut:allow-register",
    "store:default"
  ]
}
